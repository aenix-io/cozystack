{
  "title": "Chart Values",
  "type": "object",
  "properties": {
    "host": {
      "type": "string",
      "description": "The hostname used to access the grafana externally (defaults to 'grafana' subdomain for the tenant host).",
      "default": ""
    },
    "metricsStorages": {
      "type": "array",
      "description": "Configuration of metrics storage instances",
      "default": "[]",
      "items": {
        "type": "object"
      }
    },
    "oncall": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean",
          "description": "Enable Grafana OnCall",
          "default": false
        }
      }
    }
  }
}
